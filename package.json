{
  "name": "muse-ui",
  "version": "3.0.0-beta.1",
  "description": "material design ui for vue2",
  "author": "myronliu347 <myronliu347@gmail.com>",
  "main": "libs/index.js",
  "scripts": {
    "dev": "parcel demo/index.html",
    "build": "rm -rf dist && rollup --config rollup.config.js && npm run package",
    "lint": "eslint --ext .js,.vue src",
    "test": "NODE_ENV=test jest -i",
    "test:coverage": "NODE_ENV=test jest -i --coverage",
    "package": "rm -rf lib && babel src -d lib && mkdir lib/styles && cp -rf src/styles/* lib/styles"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "avoriaz": "^6.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.18.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^4.3.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "jest-serializer-html": "^5.0.0",
    "less": "^3.0.1",
    "parcel-bundler": "^1.7.1",
    "parcel-plugin-vue": "^1.5.0",
    "postcss-modules": "^1.1.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "vue": "^2.5.13",
    "vue-jest": "^2.3.0",
    "vue-template-compiler": "^2.5.13"
  },
  "browserslist": [
    "> 1%",
    "last 5 versions",
    "ios >= 7",
    "android > 4.4",
    "not ie <= 10"
  ],
  "dependencies": {
    "keycode": "^2.1.9",
    "normalize-wheel": "^1.0.1"
  },
  "jest": {
    "verbose": false,
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "\\.(less)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules",
      ".*\\.(vue|js)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-router)"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  }
}
